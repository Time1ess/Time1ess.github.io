---
layout: null
---
{
	"code" : 0,
	"data" : [{% for post in site.posts %}
	{
		"title": "{{ post.title }}",
		"tags" : [{% for tag in post.tags %}"{{tag}}"{% if forloop.rindex != 1 %},{% else %}{% endif %}{% endfor %}],
		"url" : "{{ post.url }}",
		"date": "{{ post.date | date: "%Y-%m-%d" }}"
	}{% if forloop.rindex != 1  %},{% endif %}{% endfor %}
	]
}